{
  "name": "@bladeski/random-selector",
  "version": "0.0.53",
  "description": "This is a lightweight, accessible and customisable random selector web component built with TypeScript and packaged with ParcelJS.",
  "author": "Jonathan Blades",
  "license": "MIT",
  "keywords": [
    "web component",
    "random",
    "selction",
    "TypeScript"
  ],
  "source": "src/components/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "npm run clean:dist && parcel build",
    "test": "jest",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:write": "prettier --write src/**/*.ts",
    "lint": "yarn prettier && yarn eslint",
    "lint:write": "yarn eslint:fix && yarn prettier:write"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@parcel/packager-ts": "2.8.2",
    "@parcel/transformer-typescript-types": "2.8.2",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/aria-query": "^5.0.1",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-plugin-sort-class-members": "^1.16.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "parcel": "^2.8.2",
    "prettier": "^2.8.3",
    "rimraf": "^4.0.7",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14"
  }
}
